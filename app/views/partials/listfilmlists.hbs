
{{#each filmlists}}
<div class="ui centered cards">
   <div class="ui card">
      <div class="image">
        <img src="{{picture}}">
      </div>
    <div class="content">
     <a class=" header">
      {{ uppercase title}}
    </a>
      <div class="metadata">
    <span class="date">Created: {{formatDate date}}</span>
</div>
      <div class="extra content">
    <a href="/filmlist/{{id}}" class="ui icon button">
      <i class="ui open folder icon "></i>
    </a>
    <a href="/dashboard/deletefilmlist/{{id}}" class="ui icon button delfilmlist">
      <i class="ui trash icon"></i>
    </a>
       
    </div>
     </div>
  </div>
    </div>
{{/each}}


  
  
